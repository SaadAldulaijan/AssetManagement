@model AssetManagement.Models.Telephone

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Telephone</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">MAC:</dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MAC)
        </dd>
        <dt class = "col-sm-2">SN:</dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SerialNo)
        </dd>
        <dt class = "col-sm-2">TAG:</dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Tag)
        </dd>
        <dt class = "col-sm-2">Is Defective</dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
        <dt class = "col-sm-2">Remark:</dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Remark)
        </dd>
        <dt class = "col-sm-2">Model:</dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SubCategory.Name)
        </dd>
        <dt class="col-sm-2">Brand:</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SubCategory.Category.Name)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-serialNo="@Model.SerialNo">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
